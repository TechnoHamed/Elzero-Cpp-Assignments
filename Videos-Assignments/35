/* We need 4 applications are required
1- Knowing if the number is even or odd
2- Find out the largest number
3- Knowing your rank by points
4- A simple calculator*/

// ========== App #1 ==========
#include <iostream>
using namespace std;
 int main(){
  int num;
  cin >> num;

  if
  (num % 2 == 0)
  {cout<<"The number"<<num<<"is Even"<<end;}
  else  
  {cout<<"The number"<<num<<"is Odd"<<end;}
  return 0;
 }
